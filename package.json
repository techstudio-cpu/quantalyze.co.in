{
  "name": "quantalyze-digital-agency",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:production": "NODE_ENV=production next build",
    "start:production": "NODE_ENV=production next start",
    "export": "next build",
    "postbuild": "next-sitemap",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "railway:build": "npm install && npm run build",
    "railway:start": "npm start",
    "deploy:railway": "npm run build && git add . && git commit -m 'Deploy to Railway' && git push",
    "build:static": "node scripts/build-static.js",
    "build:serverbyt": "set RAILWAY_API_URL=https://quantalyze.up.railway.app&& node scripts/build-static.js",
    "build:netlify": "node scripts/build-netlify.js"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "bcryptjs": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.469.0",
    "mysql2": "^3.15.3",
    "next": "^15.1.3",
    "next-seo": "^6.6.0",
    "next-sitemap": "^4.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-ga4": "^2.1.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "react-scroll": "^1.9.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/react-scroll": "^1.8.10",
    "@types/sqlite3": "^3.1.11",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.5.0"
}
